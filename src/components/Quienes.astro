---
const { quienesSomos } = Astro.props;
const { titulo, texto } = quienesSomos;
---

<div class="container">
  <h1 class="text-2xl font-semibold">{titulo}</h1>
  <p class="text-sm text-pretty">
    {texto}
  </p>
</div>

<style>
  .container {
    display:flex;
    flex-direction: column;
    place-items: center;
    padding: 1rem;
  }
</style>